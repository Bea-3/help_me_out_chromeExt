{
    "manifest_version": 3,
    "name": "HelpMeOut",
    "description": "This extension helps you record and share help videos with ease.",
    "icons":{
        "128": "./images/icons/iCON 7 2.png"
    },
    "background":{
        "service_worker": "background.js"
    },
    "action":{
        "default_popup": "popup.html",
        "default_icon":{
            "16": "./images/icons/iCON 7 2.png",
            "48": "./images/icons/iCON 7 2.png",
            "128": "./images/icons/iCON 7 2.png"
        }
    },
    "host_permissions":[
        "http://*",
        "https://*/*"
    ],
    "permissions":[
        "scripting",
        "tabs",
        "activeTab"
    ],
    "version": "1.0.0"

}